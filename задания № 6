#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n = 3;        // nombre d'objets
    int W = 5;        // capacit√© du sac

    vector<int> weight = {2, 3, 4};
    vector<int> value  = {3, 4, 5};

    vector<vector<int>> dp(n + 1, vector<int>(W + 1, 0));

    for (int i = 1; i <= n; i++) {
        for (int w = 0; w <= W; w++) {
            dp[i][w] = dp[i - 1][w];
            if (weight[i - 1] <= w) {
                dp[i][w] = max(dp[i][w],
                               dp[i - 1][w - weight[i - 1]] + value[i - 1]);
            }
        }
    }

    cout << "Valeur maximale = " << dp[n][W];
    return 0;
}