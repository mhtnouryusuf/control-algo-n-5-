#include <iostream>
#include <vector>
using namespace std;

int hashFunction(int x) {
    return x % 7;
}

int main() {
    vector<int> data = {86, 90, 27, 29, 38, 30, 40};
    vector<vector<int>> hashTable(7);

    for (int x : data) {
        int index = hashFunction(x);
        hashTable[index].push_back(x);
    }

    cout << "Table de hachage :" << endl;
    for (int i = 0; i < 7; i++) {
        cout << i << " : ";
        for (int v : hashTable[i]) {
            cout << v << " ";
        }
        cout << endl;
    }

    return 0;
}