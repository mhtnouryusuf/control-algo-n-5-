#include <iostream>
#include <unordered_map>
using namespace std;

char firstUniqueChar(string s) {
    unordered_map<char, int> freq;

    // Compter les occurrences
    for (char c : s) {
        freq[c]++;
    }

    // Trouver le premier caractère non répété
    for (char c : s) {
        if (freq[c] == 1) {
            return c;
        }
    }

    return '\0'; // aucun caractère unique
}

int main() {
    // Entrée intégrée
    string s = "minimum";

    char result = firstUniqueChar(s);

    // Sortie
    if (result != '\0')
        cout << "Premier caractere non repete : " << result << endl;
    else
        cout << "Aucun caractere non repete" << endl;

    return 0;
}